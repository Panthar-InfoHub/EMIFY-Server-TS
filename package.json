{
  "name": "emify-backend-ts",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently --kill-others --names \"SWC,APP\" --prefix-colors \"blue,green\" \"npm run build:watch\" \"npm run serve\"",
    "build:watch": "npx swc ./src -d ./dist --watch --copy-files --include-dotfiles --sync",
    "serve": "node ./dist/index.js"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.1",
    "@prisma/client": "6.12.0",
    "@types/joi": "^17.2.2",
    "chokidar": "3.6.0",
    "cors": "2.8.5",
    "dotenv": "17.2.0",
    "express": "5.1.0",
    "joi": "^17.13.3",
    "swc": "1.0.11",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/dotenv": "6.1.1",
    "@types/express": "5.0.3",
    "concurrently": "^8.2.2",
    "prisma": "6.12.0",
    "typescript": "5.5.3"
  }
}
