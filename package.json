{
  "name": "emify-backend-ts",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "npx swc ./src -d ./dist --copy-files --include-dotfiles",
    "dev": "concurrently --kill-others --names \"SWC,APP\" --prefix-colors \"blue,green\" \"npm run build:watch\" \"npm run serve:watch\"",
    "build:watch": "npx swc ./src -d ./dist --watch --copy-files --include-dotfiles --sync",
    "serve": "node ./dist/index.js",
    "serve:watch": "nodemon"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.1",
    "@prisma/client": "6.12.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "chokidar": "3.6.0",
    "cors": "2.8.5",
    "dotenv": "17.2.0",
    "express": "5.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "swc": "1.0.11",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/cors": "2.8.19",
    "@types/dotenv": "6.1.1",
    "@types/express": "5.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^9.32.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "nodemon": "^3.1.10",
    "prisma": "6.12.0",
    "typescript": "5.5.3",
    "typescript-eslint": "^8.38.0"
  }
}
