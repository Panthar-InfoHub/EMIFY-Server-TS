{
                                                                                     "$schema": "https://swc.rs/schema.json",
                                                                                     "sourceMaps": true,
                                                                                     "exclude": [
                                                                                       "src/prisma"
                                                                                     ],
                                                                                     "module": {
                                                                                       "type": "nodenext"
                                                                                     },
                                                                                     "jsc": {
                                                                                       "target": "esnext",
                                                                                       "parser": {
                                                                                         "syntax": "typescript",
                                                                                         "tsx": true,
                                                                                         "decorators": true,
                                                                                         "dynamicImport": true
                                                                                       },
                                                                                       "transform": {
                                                                                         "legacyDecorator": true,
                                                                                         "decoratorMetadata": true,
                                                                                         "react": {
                                                                                           "throwIfNamespace": false,
                                                                                           "useBuiltins": false,
                                                                                           "pragma": "React.createElement",
                                                                                           "pragmaFrag": "React.Fragment",
                                                                                           "importSource": "react"
                                                                                         }
                                                                                       },
                                                                                       "keepClassNames": true,
                                                                                       "baseUrl": "./src",
                                                                                       "paths": {
                                                                                         "@/*": ["*"]
                                                                                       }
                                                                                     }
                                                                                   }